{
    "restartable": "rs",
    "legacy-watch": true,
    "cleanly": true,
    "ignore": [
        ".git",
        "node_modules/**/node_modules",
        "./website"
    ],
    "verbose": true,
    "execMap": {
        "": "node" 
    },
    "exec": "node ./example/index",
    "events": {},
    "watch": [
        "example/**/*.js",
        "lib/**/*.js" 
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js json"
}